# Conway's Game of Life with Animation
# Rules:
# 1. Any live cell with fewer than two live neighbors dies
# 2. Any live cell with two or three live neighbors lives on
# 3. Any live cell with more than three live neighbors dies
# 4. Any dead cell with exactly three live neighbors becomes a live cell

def sum(arr):
  fold(arr, 0, fn(acc, x): acc + x;);

def safe_get_1d(row, col):
  if (col >= 0 && col < len(row)):
    row[col]
  else:
    0
end

def safe_get_2d(grid, row, col):
  if (row >= 0 && row < len(grid)):
    safe_get_1d(grid[row], col)
  else:
    0
end

def count_neighbors(grid, row, col):
  [
    safe_get_2d(grid, row - 1, col - 1),
    safe_get_2d(grid, row - 1, col),
    safe_get_2d(grid, row - 1, col + 1),
    safe_get_2d(grid, row, col - 1),
    safe_get_2d(grid, row, col + 1),
    safe_get_2d(grid, row + 1, col - 1),
    safe_get_2d(grid, row + 1, col),
    safe_get_2d(grid, row + 1, col + 1)
  ]
  | sum();

def apply_rules(current_cell, neighbor_count):
  if (current_cell == 1):
    if (neighbor_count < 2): 0
    elif (neighbor_count == 2 || neighbor_count == 3): 1
    else: 0
  else:
    if (neighbor_count == 3): 1
    else: 0
end

def next_generation(grid):
  let height = len(grid)
  | let width = len(grid[0])
  | map(range(0, height - 1, 1),
  fn(row):
    map(range(0, width - 1, 1),
      fn(col):
        let current_cell = safe_get_2d(grid, row, col)
        | let neighbor_count = count_neighbors(grid, row, col)
        | apply_rules(current_cell, neighbor_count);
    );
)
end

def grid_to_string(grid):
  map(grid,
    fn(row):
      map(row, fn(cell): if (cell == 1): "â–ˆ" else: " ";) | join("");
  ) | join("\n")
end

def run_game_of_life(initial_grid, generations):
  let current_grid = initial_grid
  | let line = repeat("=", 40)
  | let i = 0
  | until (i <= generations):
      # Clear screen using ANSI escape sequence
      print("\x1b[2J\x1b[H")
      | let index = to_string(i)
      | let grid = grid_to_string(current_grid)
      # Print generation counter
      | print(s"Generation: ${index}
${line}
${grid}
${line}")
      # Calculate next generation
      | let current_grid = next_generation(current_grid)
      | let i = i + 1
      | current_grid;
end

